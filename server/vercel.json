{
    "version": 2,
    "builds": [
        {
            "src": "djangoProject/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "staticfiles/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "djangoProject/wsgi.py"
        }
    ]
}